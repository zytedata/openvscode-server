{
	"name": "gitpod-desktop",
	"displayName": "%displayName%",
	"description": "%description%",
	"publisher": "gitpod",
	"version": "0.0.29",
	"license": "MIT",
	"preview": true,
	"icon": "resources/gitpod.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gitpod-io/vscode.git"
	},
	"bugs": {
		"url": "https://github.com/gitpod-io/gitpod/issues"
	},
	"engines": {
		"vscode": "^1.58.2"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"remote development",
		"remote"
	],
	"extensionKind": [
		"ui"
	],
	"activationEvents": [
		"*",
		"onCommand:gitpod.api.autoTunnel",
		"onCommand:gitpod.api.auth",
		"onAuthenticationRequest:gitpod"
	],
	"contributes": {
		"authentication": [
			{
				"label": "Gitpod",
				"id": "gitpod"
			}
		],
		"configuration": {
			"title": "Settings sync",
			"properties": {
				"configurationSync.store": {
					"type": "object"
				}
			}
		},
		"commands": [
			{
				"command": "gitpod.auth.remove",
				"category": "Gitpod",
				"enablement": "gitpod.addedSyncProvider",
				"title": "Turn off Settings Sync"
			},
			{
				"command": "gitpod.auth.add",
				"category": "Gitpod",
				"enablement": "!gitpod.addedSyncProvider",
				"title": "Turn on Settings Sync"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"compile": "gulp compile-extension:gitpod",
		"watch": "gulp watch-extension:gitpod",
		"vscode:prepublish": "npm run compile"
	},
	"devDependencies": {
		"@types/node": "16.x",
		"@types/node-fetch": "^2.5.12",
		"@types/tmp": "^0.2.1"
	},
	"dependencies": {
		"@gitpod/gitpod-protocol": "main",
		"@gitpod/local-app-api-grpcweb": "main",
		"@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
		"node-fetch": "2.6.7",
		"tmp": "^0.2.1",
		"vscode-nls": "^5.0.0"
	},
	"extensionDependencies": [
		"ms-vscode-remote.remote-ssh"
	]
}
